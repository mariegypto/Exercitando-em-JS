//Faça um programa que leia 2 números, some-os e exiba uma mensagem com o resultado apenas se o resultado for maior que 100.

var v1, v2, soma;
v1 = parseFloat(prompt("Insira o primeiro número"));
v2 = parseFloat(prompt("Insira o segundo número"))
soma = v1 + v2;
if (soma > 100) {
   alert("O resultado é:" + soma)
}
